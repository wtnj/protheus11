User Function MDTA5401()

	Local aCores := {{ "BR_VERDE"    , "Extintor Habilitado" },;
                     { "BR_VERMELHO" , "Extintor c/ Validade Vencida" }}

	Local uRetorno := {}
	
	Aadd(uRetorno, { 'TLA_SITUAC == "A"' , aCores[1][1] } )
	Aadd(uRetorno, { 'TLA_SITUAC <> "A"' , aCores[2][1] } )

Return(uRetorno)

