Local lOK       := .F.

IF M->B1_TIPO == "FE"

	SX5->(DbGotop())
	SX5->(DbSetOrder(1))
	SX5->(DbSeek(xFilial("SX5")+"ZF")) // Tabela de validação de Usuarios, para alterar o produto por FOLDER
	While !SX5->(Eof()) .And. SX5->X5_TABELA == "ZF"
	   If Alltrim(Substr(SX5->X5_DESCRI,1,16)) == Upper(Alltrim(Subs(cUsuario,7,15))) //Verifica se o Usuario é o mesmo da tabela ZF no SX5
	      lOK := .T. //Flag de Controle          
	      Exit //achou força a saida
	   Endif
	   SX5->(DbSkip())
	Enddo

	Return(lOk)  // retorna
ENDIF

Return(.T.)