Static Function fCriaCor()       

Local aLegenda :=	{ {"BR_VERDE"   , OemToAnsi("Ação Implementada com Sucesso")  },;
                      {"BR_LARANJA" , OemToAnsi("Ação não Implementada") },;                      
  					  {"BR_AZUL"    , OemToAnsi("Ação Ineficaz")   }}

