oDlg1      := MSDialog():New( 091,232,691,927,"oDlg1",,,.F.,,,,,,.T.,,,.T. )
MHoBrw1()
MCoBrw1()
oBrw1      := MsNewGetDados():New(004,000,292,340,nOpc,'AllwaysTrue()','AllwaysTrue()','',,0,99,'AllwaysTrue()','','AllwaysTrue()',oDlg1,aHoBrw1,aCoBrw1 )

