@ nLin , 0 PSAY "*" + Repl("-",130) + "*"
nLin := nLin + 1
@ nLin , 0 PSAY STR0023 //" 1¦VIA - INSS - 2¦VIA - CONTRIBUINTE"
nLin := nLin + 1

Return Nil

