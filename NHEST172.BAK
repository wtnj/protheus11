		While !Eof() .and. (aFilial[ F_SB9 ]+cCod+cLocal == B9_FILIAL + B9_COD+B9_Local)
			If B9_DATA >= dData .and. lHasRec
				Exit
			Else
				lHasRec := .t.
			EndIf
			DbSkip()
		End
		If lHasRec
			DbSkip(-1)
		EndIf
	End
