				RecLock("ZRC",.T.)
				ZRC->ZRC_FILIAL := xFilial("ZRC")
				ZRC->ZRC_MAT    := ZRA->ZRA_MAT
				ZRC->ZRC_NOME   := ZRA->ZRA_NOME
				ZRC->ZRC_PD     := "502"
				ZRC->ZRC_DESCPD := "COFINS"
				ZRC->ZRC_TIPO1  := "V"
				ZRC->ZRC_VALOR  := ((_nRaSal * ZRA->ZRA_PERCOF)/100)
