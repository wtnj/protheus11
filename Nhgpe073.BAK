				ZRC->ZRC_NOME   := ZRA->ZRA_NOME
				ZRC->ZRC_PD     := "501"
				ZRC->ZRC_DESCPD := "PIS"
				ZRC->ZRC_TIPO1  := "V"
				ZRC->ZRC_VALOR  := ((_nRaSal * ZRA->ZRA_PERPIS)/100)
				ZRC->ZRC_DATA   := dDataBase
				ZRC->ZRC_CC     := ZRA->ZRA_CCUSTO
