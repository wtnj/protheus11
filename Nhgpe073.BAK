Static Function fGravaFin()
	ZRC->(DbSeek(xFilial("ZRC") +ZRA->ZRA_MAT ))
	While !ZRC->(Eof()) .AND. ZRC->ZRC_MAT == ZRA->ZRA_MAT

		RecLock("ZRC",.F.)
		ZRC->ZRC_PREFIX := _cPrefixo      
		ZRC->ZRC_NUMERO := _cNumero
		ZRC->ZRC_EMISSA := _dEmissao
		ZRC->ZRC_VENCTO := _dVencto
		DbUnLock("ZRC")
		ZRC->(DbSkip())	

	Enddo
	Close(oDlg)

Return


