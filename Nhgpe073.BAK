		If ZRA->ZRA_PERIRF > 0 // IR Verba: 504
			ZRC->(DbSeek(xFilial("ZRC") +ZRA->ZRA_MAT + "504"))
			If !ZRC->(Found())
				RecLock("ZRC",.T.)
				ZRC->ZRC_FILIAL := xFilial("ZRC")
				ZRC->ZRC_MAT    := ZRA->ZRA_MAT
				ZRC->ZRC_NOME   := ZRA->ZRA_NOME
				ZRC->ZRC_PD     := "504"
				ZRC->ZRC_DESCPD := "I.R"
				ZRC->ZRC_TIPO1  := "V"
				ZRC->ZRC_VALOR  := ((_nRaSal * ZRA->ZRA_PERIRF)/100)
				ZRC->ZRC_DATA   := dDataBase
				ZRC->ZRC_CC     := ZRA->ZRA_CCUSTO
				ZRC->ZRC_TIPO2  := "C"
				DbUnLock("ZRC")
			Else
				RecLock("ZRC",.F.)
				ZRC->ZRC_FILIAL := xFilial("ZRC")
				ZRC->ZRC_MAT    := ZRA->ZRA_MAT
				ZRC->ZRC_NOME   := ZRA->ZRA_NOME
				ZRC->ZRC_PD     := "504"
				ZRC->ZRC_DESCPD := "I.R"
				ZRC->ZRC_TIPO1  := "V"
				ZRC->ZRC_VALOR  := ((_nRaSal * ZRA->ZRA_PERIRF)/100)
				ZRC->ZRC_DATA   := dDataBase
				ZRC->ZRC_CC     := ZRA->ZRA_CCUSTO
				ZRC->ZRC_TIPO2  := "C"
				DbUnLock("ZRC")
			Endif
        Endif

		_nTotpro := _nTotdes := 0
		// Totaliza Proventos/Descontos
		ZRC->(DbSeek(xFilial("ZRC") +ZRA->ZRA_MAT))
		While !ZRC->(Eof()) .AND. ZRC->ZRC_MAT == ZRA->ZRA_MAT

			If ZRC->ZRC_PD == '799'
				ZRC->(DbSkip())
				Loop
			Endif	
			// Pesquisa verba 1 - Provento 2 - Desconto				
			ZRV->(DbSeek(xFilial("ZRV") + ZRC->ZRC_PD))
			If ZRV->(Found())
				_cPd := ZRV->ZRV_TIPOCO
			Endif
			If _cPd == "1"
				_nTotpro += ZRC->ZRC_VALOR
			Elseif 	_cPd == "2" .AND. ZRC->ZRC_TIPO2 <> "I"
				_nTotdes += ZRC->ZRC_VALOR
			Endif
			ZRC->(DbSkip())
		Enddo

		// Grava Liquido
		ZRC->(DbSeek(xFilial("ZRC") +ZRA->ZRA_MAT + "799"))
		If !ZRC->(Found())
			RecLock("ZRC",.T.)
			ZRC->ZRC_FILIAL := xFilial("ZRC")
			ZRC->ZRC_MAT    := ZRA->ZRA_MAT
			ZRC->ZRC_NOME   := ZRA->ZRA_NOME
			ZRC->ZRC_PD     := "799"
			ZRC->ZRC_DESCPD := "LIQUIDO A RECEBER"
			ZRC->ZRC_TIPO1  := "V"
			ZRC->ZRC_VALOR  :=(_nTotpro  - _nTotdes)
			ZRC->ZRC_DATA   := dDataBase
			ZRC->ZRC_CC     := ZRA->ZRA_CCUSTO
			ZRC->ZRC_TIPO2  := "C"				
			DbUnLock("ZRC")
		Else
			RecLock("ZRC",.F.)
			ZRC->ZRC_FILIAL := xFilial("ZRC")
			ZRC->ZRC_MAT    := ZRA->ZRA_MAT
			ZRC->ZRC_NOME   := ZRA->ZRA_NOME
			ZRC->ZRC_PD     := "799"
			ZRC->ZRC_DESCPD := "LIQUIDO A RECEBER"
			ZRC->ZRC_TIPO1  := "V"
			ZRC->ZRC_VALOR  :=(_nTotpro  - _nTotdes)
			ZRC->ZRC_DATA   := dDataBase
			ZRC->ZRC_CC     := ZRA->ZRA_CCUSTO
			ZRC->ZRC_TIPO2  := "C"				
			DbUnLock("ZRC")
		Endif
		DbSelectArea("ZRA")
		ZRA->(DbSkip())
	Enddo                                                                                                    
                                                   
Endif

Return(.T.)


User function fGerLiq()

If MsgBox("Confirma geracao dos liquidos ?","Gera liquidos","YESNO")

	Pergunte("NHGP073")
	
	DbSelectArea("ZRC")
	ZRC->(DbGotop())
	While !ZRC->(Eof())
		If Mv_par01 == 1
			If ZRC->ZRC_PD == '799' 
				ZR1->(DbSeek(xFilial("ZR1") +ZRC->ZRC_MAT+DTOS(dDataBase)))
				If !ZR1->(Found())
					RecLock("ZR1",.T.)
					ZR1->ZR1_FILIAL := xFilial("ZRC")
					ZR1->ZR1_MAT    := ZRC->ZRC_MAT
					ZR1->ZR1_TIPO   := "1"
					ZR1->ZR1_VALOR  := ZRC->ZRC_VALOR              
					ZR1->ZR1_DATA   := dDataBase
					DbUnLock("ZR1")
				Endif
			Endif		
		Elseif Mv_par01 == 2
			If ZRC->ZRC_PD == '508'
				ZR1->(DbSeek(xFilial("ZR1") +ZRC->ZRC_MAT+DTOS(dDataBase)))
				If !ZR1->(Found())
					RecLock("ZR1",.T.)
					ZR1->ZR1_FILIAL := xFilial("ZRC")
					ZR1->ZR1_MAT    := ZRC->ZRC_MAT
					ZR1->ZR1_TIPO   := "2"
					ZR1->ZR1_VALOR  := ZRC->ZRC_VALOR              
					ZR1->ZR1_DATA   := dDataBase
					DbUnLock("ZR1")
				Endif
			Endif		
 		Endif	
		ZRC->(DbSkip())
	Enddo
Endif	

Return(.t.)
