	//  Folder 4 - FormataÁ„o Final  --------------       		 
	//  Carrega a descriÁ„o do Fornecedor4
	 	SA2->(DBSETORDER(1))
		SA2->(DBSEEK(xFilial("SA2") + CForn4))
