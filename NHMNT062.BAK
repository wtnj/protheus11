	If  !Empty(AllTrim(cMaq4))	
		ST9->(DBSETORDER(1))
		ST9->(DBSEEK(xFilial("ST9") + AllTrim(cMaq4)))	   
		DescMaq4 := AllTrim(ST9->T9_NOME)
