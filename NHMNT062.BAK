	if VAL(SUBSTR(AllTrim(cCombo),1,1)) >= 6 .OR. cRegrav == 'S'
		Load('6') // Carrega os dados do GetDados da Nitretacao		
	endif
	
	if VAL(SUBSTR(AllTrim(cCombo),1,1)) >= 7 .OR. cRegrav == 'S'
